{
	"name": "kysely-bun-sql",
	"version": "0.2.0",
	"description": "Kysely dialect/driver for PostgreSQL using Bun's native SQL",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist/",
		"README.md",
		"LICENSE.txt"
	],
	"sideEffects": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lacion/kysely-bun-sql.git"
	},
	"bugs": "https://github.com/lacion/kysely-bun-sql/issues",
	"author": {
		"name": "Luis Morales"
	},
	"engines": {
		"bun": ">=1.3.0"
	},
	"scripts": {
		"test": "bun test --coverage",
		"lint": "bunx biome lint .",
		"format": "bunx biome format --write .",
		"typecheck": "bunx tsc -p tsconfig.json --noEmit",
		"build": "bunx tsc -p tsconfig.build.json",
		"bundle": "bun build ./src/index.ts --outdir ./dist"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"@biomejs/biome": "^2.2.4",
		"kysely": "^0.28.8"
	},
	"peerDependencies": {
		"typescript": "^5",
		"kysely": "^0.28.8"
	},
	"dependencies": {},
	"publishConfig": {
		"access": "public"
	}
}
